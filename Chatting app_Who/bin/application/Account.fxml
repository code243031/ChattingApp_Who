<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox minHeight="800.0" minWidth="650.0" prefHeight="800.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AccountController">
   <children>
      <Label text="회원 가입" textAlignment="CENTER" underline="true">
         <VBox.margin>
            <Insets bottom="15.0" right="15.0" top="15.0" />
         </VBox.margin>
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Separator prefHeight="50.0" prefWidth="550.0" />
      <HBox prefHeight="0.0" prefWidth="550.0">
         <children>
            <Label minHeight="44.0" minWidth="136.0" text="ID">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="accountID" prefHeight="44.0" prefWidth="225.0">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </TextField>
            <Button fx:id="chkEquals" mnemonicParsing="false" text="Button">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox prefHeight="0.0" prefWidth="550.0">
         <children>
            <Label minHeight="44.0" minWidth="136.0" text="Password">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="accountPW" prefHeight="44.0" prefWidth="250.0">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox prefHeight="0.0" prefWidth="550.0">
         <children>
            <Label minHeight="44.0" minWidth="136.0" text="Pw again">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="PwAgain" prefHeight="44.0" prefWidth="250.0">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <Label fx:id="PwEquals" />
      <HBox prefHeight="0.0" prefWidth="550.0">
         <children>
            <Label minHeight="44.0" minWidth="136.0" text="이름(닉네임)">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="accountName" prefHeight="44.0" prefWidth="250.0">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <Separator prefHeight="50.0" prefWidth="550.0" />
      <TextArea editable="false" prefHeight="200.0" prefWidth="200.0" text="회원가입을 통해 만들어지는 계정은 실제 정보를 입력하지 않으셔도 됩니다.&#10;(가입 약관 같은거 적는 공간 만들기 위해 놓은 위젯입니다.)" wrapText="true" />
      <HBox prefHeight="0.0" prefWidth="550.0">
         <children>
            <RadioButton fx:id="agree" mnemonicParsing="false" text="동의">
               <HBox.margin>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </HBox.margin>
               <toggleGroup>
                  <ToggleGroup fx:id="isAgree" />
               </toggleGroup>
            </RadioButton>
            <RadioButton fx:id="disagree" mnemonicParsing="false" selected="true" text="동의하지 않음" toggleGroup="$isAgree">
               <HBox.margin>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </HBox.margin>
            </RadioButton>
         </children>
      </HBox>
      <HBox alignment="TOP_RIGHT" prefHeight="0.0" prefWidth="550.0">
         <children>
            <Button fx:id="request" disable="true" mnemonicParsing="false" text="가입 신청">
               <HBox.margin>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="cancel" mnemonicParsing="false" text="취소">
               <HBox.margin>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
   </padding>
</VBox>
